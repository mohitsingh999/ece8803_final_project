                        # Resolve the collision
                        if self.vx > 0:
                            self.x = block_rect.left - self.width
                        elif self.vx < 0:
                            self.x = block_rect.right
                        if self.vy > 0:
                            self.y = block_rect.top - self.height
                            self.vy = 0
                        elif self.vy < 0:
                            self.y = block_rect.bottom

    def draw(self, screen):
        # Draw the player
        screen.blit(self.image, (self.x, self.y))

